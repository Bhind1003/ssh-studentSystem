<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hib.StatusChangeEntity" table="status_change" schema="student">
        <id name="changeno" column="changeno"/>
        <property name="sno" column="sno"/>
        <property name="changeName" column="change_name"/>
        <property name="date" column="date"/>
        <property name="detail" column="detail"/>
    </class>
</hibernate-mapping>